<div class="container">
  <h4 class="mt-3">ジャンル編集</h4>
    <div class="row">
      <div class="mx-auto col-6" >
        <%= form_with model: @genre, url: admin_genre_path do |f| %>
          <div class="form-group">
            <lavel for="genre_name">ジャンル名</lavel>
            <%= f.text_field :name %>
            <%= f.submit "変更を保存", class: "btn btn-success" %>
          </div>
        <% end %>
      </div>
    </div>
</div>
      
 